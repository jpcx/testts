{
  "name": "@jpcx/testts",
  "version": "0.1.2",
  "description": "an asynchronous, nesting, composable testing framework for TypeScript [zero-dependency]",
  "license": "GPL-3.0-or-later",
  "repository": "git://github.com/jpcx/testts.git",
  "author": {
    "name": "Justin Collier",
    "email": "m@jpcx.dev",
    "url": "https://github.com/jpcx"
  },
  "keywords": [
    "test",
    "unittest",
    "unit-test",
    "small",
    "tiny",
    "asynchronous",
    "nesting",
    "zero-dependency",
    "single-file"
  ],
  "main": "testts.js",
  "types": "testts.d.ts",
  "scripts": {
    "clean": "rimraf testts.js testts.d.ts",
    "clean-test": "rimraf test/dist",
    "build-test": "tsc -p test",
    "test": "./test/node_modules/.bin/testts test/dist",
    "prepublishOnly": "npm run-script build",
    "prebuild": "npm run-script clean && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty && chmod a+x testts.js",
    "watch": "npm run-script build -- --watch"
  },
  "bin": {
    "testts": "./testts.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": ">=10.17.35",
    "typescript": ">=3.6",
    "rimraf": "3.*"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
